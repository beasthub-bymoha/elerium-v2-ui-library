--functions
function autopunch()
while getgenv().AutoPunch do task.wait()
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SkillRemote"):FireServer({SkillId = "1", Began = true, ["Typ\208\181"] = 1})
end
end

function autopunch2()
while getgenv().AutoPunch2 do task.wait()
require(game:GetService("ReplicatedStorage").Packages.Knit).GetController("SkillManager").TapInput({SkillId = "1", Began = true})
end
end

function autopunch3()
while getgenv().AutoPunch3 do task.wait()
require(game:GetService("ReplicatedStorage").Packages._Index["sleitnick_knit@1.4.7"].knit).GetController("SkillManager").TapInput({SkillId = "1", Began = true})
end
end

function autoblock()
    while getgenv().AutoBlock do
        game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.B, false, game)
        task.wait()
    end
    game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.B, false, game)
end

function autoagility()
while getgenv().AutoAgility do task.wait()
game:GetService("Players").LocalPlayer.Character.Humanoid:Move(Vector3.new(0, 0, -1), true)
end
end

function autoenergy()
    while getgenv().AutoEnergy do
        game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.C, false, game)
        task.wait()
    end
    game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.C, false, game)
end

function autoenergyball()
while getgenv().AutoEnergyBall do task.wait()
require(game:GetService("ReplicatedStorage").Packages.Knit).GetController("SkillManager").TapInput({SkillId = "101", Began = true})
end
end

