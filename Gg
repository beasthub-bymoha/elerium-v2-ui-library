Tab:AddToggle({
    Name = "Kill From Anywhere Puriza X003 Boss Mobs (New Method)",
    Default = false,
    Save = true,
    Flag = "autokillfromanywherepurizax003bossmobs_db",
    Callback = function(Value)
        getgenv().autokillfromanywherepurizax003bossmobs = Value
        if not Value then return end
        task.spawn(function()
            local baseplate = Instance.new("Part")
            baseplate.Size = Vector3.new(20, 1, 20)
            baseplate.Anchored = true
            baseplate.CanCollide = true
            baseplate.Transparency = 1
            baseplate.Parent = game:GetService("Workspace")
            while getgenv().autokillfromanywherepurizax003bossmobs do
                local mobsFolder = game:GetService("Workspace")["World Mobs"]:FindFirstChild("Boss Mobs")
                if mobsFolder then
                    for i, v in next, mobsFolder:GetChildren() do
                        if not getgenv().autokillfromanywherepurizax003bossmobs then break end
                        if v:IsA("Model") and v.Name == "Puriza X003" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChildWhichIsA("Humanoid") and v:FindFirstChildWhichIsA("Humanoid").Health > 0 then
                            repeat
                                if not getgenv().autokillfromanywherepurizax003bossmobs then break end
                                local mobsFolderCheck = game:GetService("Workspace")["World Mobs"]:FindFirstChild("Boss Mobs")
                                if not mobsFolderCheck or not mobsFolderCheck:FindFirstChild(v.Name) then
                                    break
                                end
                                local currentMob = mobsFolderCheck:FindFirstChild(v.Name)
                                local originalPos = currentMob:GetPivot()
                                if game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                    local playerPos = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
                                    currentMob:PivotTo(CFrame.new(playerPos) * CFrame.new(0, -7.5, 0))
                                    baseplate.Position = playerPos - Vector3.new(0, 3, 0)
                                    currentMob.HumanoidRootPart.Anchored = true
                                    if currentMob.Humanoid:FindFirstChild("Animator") then 
                                        currentMob.Humanoid.Animator:Destroy() 
                                    end
                                end
                                task.wait()
                            until not currentMob or currentMob:FindFirstChildWhichIsA("Humanoid").Health <= 0 or not getgenv().autokillfromanywherepurizax003bossmobs
                            if currentMob and currentMob:FindFirstChild("HumanoidRootPart") then
                                currentMob.HumanoidRootPart.Anchored = false
                            end
                        end
                    end
                end
                if game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    baseplate.Position = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position - Vector3.new(0, 3, 0)
                end
                task.wait()
            end
            baseplate:Destroy()
            local mobs = game:GetService("Workspace")["World Mobs"]:FindFirstChild("Boss Mobs")
            if mobs then
                for i, v in next, mobs:GetChildren() do
                    if v:FindFirstChild("HumanoidRootPart") then
                        v.HumanoidRootPart.Anchored = false
                    end
                end
            end
        end)
    end
})
