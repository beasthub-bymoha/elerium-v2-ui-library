end

function autorebirth()
while getgenv().AutoRebirth do task.wait()

for i, rb in ipairs(game:GetService("Players").LocalPlayer.PlayerGui.MainGui.Frame.Stats.AspectRatioFrame.StatHUD.Frame:GetChildren()) do
if rb:FindFirstChild("RebirthButton") and rb:FindFirstChild("RebirthButton").BackgroundTransparency ~= 1 then
game:GetService("ReplicatedStorage"):WaitForChild("Packages"):WaitForChild("_Index"):WaitForChild("sleitnick_knit@1.4.7"):WaitForChild("knit"):WaitForChild("Services"):WaitForChild("PlayerLevelService"):WaitForChild("RF"):WaitForChild("RequestRebirth"):InvokeServer(true)
end
end
end
end

function autorebirth2()
while getgenv().AutoRebirth2 do task.wait()

for i, rb in ipairs(game:GetService("Players").LocalPlayer.PlayerGui.MainGui.Frame.Stats.AspectRatioFrame.StatHUD.Frame:GetChildren()) do
if rb:FindFirstChild("RebirthButton") and rb:FindFirstChild("RebirthButton").BackgroundTransparency ~= 1 then
require(game:GetService("ReplicatedStorage").Packages.Knit).GetService("PlayerLevelService"):RequestRebirth(true)
end
end
end
end

function autorebirth3()
while getgenv().AutoRebirth3 do task.wait()

for i, rb in ipairs(game:GetService("Players").LocalPlayer.PlayerGui.MainGui.Frame.Stats.AspectRatioFrame.StatHUD.Frame:GetChildren()) do
if rb:FindFirstChild("RebirthButton") and rb:FindFirstChild("RebirthButton").BackgroundTransparency ~= 1 then
require(game:GetService("ReplicatedStorage").Packages._Index["sleitnick_knit@1.4.7"].knit).GetService("PlayerLevelService"):RequestRebirth(true)
end
end
end
end

function autodrops()
    for i, v in pairs(game:GetService("ReplicatedStorage").Data.ItemDrops:GetChildren()) do
        if v:GetAttribute("UserId") == game:GetService("Players").LocalPlayer.UserId and v:GetAttribute("ItemName") then
            local ops = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
            local pos = v:GetAttribute("Pos")
            game:GetService("Players").LocalPlayer.Character:MoveTo(pos)
            task.wait(0.5)
            require(game:GetService("ReplicatedStorage").Packages.Knit).GetController("ItemDropController"):ClaimItem(v.Name, require(game:GetService("ReplicatedStorage").Packages.Knit).GetController("InventoryController"):GetItem(v:GetAttribute("ItemName")))
            task.wait(0.5)
            game:GetService("Players").LocalPlayer.Character:MoveTo(ops)
        end
    end
end

function autodrops2()
    while getgenv().AutoDrops2 do
        task.wait()
        for i, v in pairs(game:GetService("ReplicatedStorage").Data.ItemDrops:GetChildren()) do
            if v:GetAttribute("UserId") == game:GetService("Players").LocalPlayer.UserId and v:GetAttribute("ItemName") then
                local ops = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
                local pos = v:GetAttribute("Pos")
                game:GetService("Players").LocalPlayer.Character:MoveTo(pos)
                task.wait(0.5)
                require(game:GetService("ReplicatedStorage").Packages._Index["sleitnick_knit@1.4.7"].knit).GetController("ItemDropController"):ClaimItem(v.Name, require(game:GetService("ReplicatedStorage").Packages._Index["sleitnick_knit@1.4.7"].knit).GetController("InventoryController"):GetItem(v:GetAttribute("ItemName")))
                task.wait(0.5)
                game:GetService("Players").LocalPlayer.Character:MoveTo(ops)
            end
        end
    end
end

function autodrops3()
    while getgenv().AutoDrops3 do
        task.wait()
        for i, v in pairs(game:GetService("ReplicatedStorage").Data.ItemDrops:GetChildren()) do
            if v:GetAttribute("UserId") == game:GetService("Players").LocalPlayer.UserId and v:GetAttribute("ItemName") then
                local ops = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
                local pos = v:GetAttribute("Pos")
                game:GetService("Players").LocalPlayer.Character:MoveTo(pos)
                task.wait(0.5)
                game:GetService("ReplicatedStorage").Packages._Index["sleitnick_knit@1.4.7"].knit.Services.ItemDropService.RF.ClaimItem:InvokeServer(v.Name, v:GetAttribute("ItemName"))
                task.wait(0.5)
                game:GetService("Players").LocalPlayer.Character:MoveTo(ops)
            end
        end
    end
end

function autodrops4()
    while getgenv().AutoDrops4 do
        task.wait()
        for i, v in pairs(game:GetService("ReplicatedStorage").Data.ItemDrops:GetChildren()) do
            if v:GetAttribute("UserId") == game:GetService("Players").LocalPlayer.UserId then
                local ops = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
                game:GetService("Players").LocalPlayer.Character:MoveTo(v:GetAttribute("Pos"))
                task.wait(0.5)
                local drop = game:GetService("Workspace"):FindFirstChild("PartStorage"):FindFirstChild("ItemDrop_"..v.Name)
                if drop and drop.Base and drop.Base:FindFirstChildOfClass("ProximityPrompt") then
                fireproximityprompt(drop.Base.ProximityPrompt)
                end
                task.wait(0.5)
                game:GetService("Players").LocalPlayer.Character:MoveTo(ops)
            end
        end
    end
end

function autodrops5()
    while getgenv().AutoDrops5 do task.wait()
        for i, v in pairs(game:GetService("ReplicatedStorage").Data.ItemDrops:GetChildren()) do
            if v:GetAttribute("UserId") == game:GetService("Players").LocalPlayer.UserId then
                local ops = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
                game:GetService("Players").LocalPlayer.Character:MoveTo(v:GetAttribute("Pos"))
                task.wait(0.5)
                local drop = game:GetService("Workspace"):FindFirstChild("PartStorage"):FindFirstChild("ItemDrop_"..v.Name)
                if drop and drop.Base and drop.Base:FindFirstChildOfClass("ProximityPrompt") then
                firesignal(drop.Base.ProximityPrompt.Triggered)
                end
                task.wait(0.5)
                game:GetService("Players").LocalPlayer.Character:MoveTo(ops)
            end
        end
    end
end

function autospin()
while getgenv().AutoSpin do task.wait()
local args = {
[1] = "SpinMain"
}

game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.4.7").knit.Services.LootPoolService.RF.SpinRequest:InvokeServer(unpack(args))
end
end

