Tab:AddButton({
Name = "Go To Quest Spawned Items",
Callback = function()
for i, v in next, game:GetService("Workspace").Misc.QuestProps:GetChildren() do
if v:IsA("Folder") then
for _, child in next, v:GetChildren() do
if child:IsA("Part") then
if child:FindFirstChildWhichIsA("Model") and child:FindFirstChildWhichIsA("Model").Parent.Name == child.Name then
local endTime = tick() + 2.5
while tick() < endTime do
game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = child.CFrame
task.wait()
end
end
end
end
end
end
end
})
