local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Hydrazia/Exploits/refs/heads/main/ORION"))()

OrionLib:MakeNotification({
Name = "Dragon Blox",
Content = "Scripted By Turbo (Updated 1/18/2026!)",
Image = "rbxassetid://4483345998",
Time = 5
})

local Window = OrionLib:MakeWindow({
Name = "Turbo | DB Enhanced Edition",
HidePremium = false,
SaveConfig = false,
ConfigFolder = "Dragon Blox",
IntroEnabled = true,
IntroText = "Dragon Blox Gui loaded world 1"
})

local TimeTab = Window:MakeTab({
Name = "Info",
Icon = "rbxassetid://4483345998",
PremiumOnly = false
})

local TimeSection = TimeTab:AddSection({
Name = "Status"
})

local ClockLabel = TimeSection:AddLabel("Current Time and Date: " .. os.date("%Y-%m-%d %I:%M:%S") .. ".000 " .. os.date("%p"))

task.spawn(function()
local lastMilliseconds = 0

while true do
local currentTime = tick()
local currentUnixTime = os.time()
local milliseconds = math.floor((currentTime % 1) * 1000)

if milliseconds ~= lastMilliseconds then
local formattedTime = os.date("%Y-%m-%d %I:%M:%S", currentUnixTime) .. "." .. string.format("%03d", milliseconds) .. " " .. os.date("%p", currentUnixTime)
ClockLabel:Set("Current Time and Date: " .. formattedTime)
lastMilliseconds = milliseconds
end

local waitTime = 0.001 - (currentTime % 0.001)
if waitTime > 0 then
task.wait(waitTime)
else
task.wait(0.0001)
end
end
end)
-- local mytable
